<p id="notice"><%= notice %></p>

<div class = "profile-center"
<h1>Team Members</h1>

<table>
  <thead>
    <tr>
      <th>Profile</th>
      <th>Team</th>
      <th>Hackathons</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @team_members.each do |team_member| %>
      <tr>
        <td><%= team_member.profile.name %></td>
        <td><%= team_member.team.name %></td>
        <td><%= team_member.team.hackathon.name %></td>
        <td><%= link_to 'Show', team_member %></td>
      </tr>
    <% end %>
  </tbody>
</table>
</div>
<br>
